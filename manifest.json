{
    "manifest_version": 3,
    "name": "QR Code Generator",
    "version": "1.0",
    "description": "Gere QR Codes rapidamente.",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "web_accessible_resources": [{
      "resources": ["qrcode.min.js"],
      "matches": ["<all_urls>"]
    }]
  }
  